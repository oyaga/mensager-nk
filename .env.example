# Database
# Local Postgres
# DATABASE_URL=postgresql://chatwoot:chatwoot123@localhost:5432/chatwoot_go?sslmode=disable
# Supabase (Transaction Pool - Porta 6543)
# DATABASE_URL=postgres://[db-user]:[db-password]@aws-0-us-east-1.pooler.supabase.com:6543/[db-name]?sslmode=require&supa=base
# Supabase (Session Pool - Porta 5432)
DATABASE_URL=postgresql://postgres:[PASSWORD]@db.[PROJECT-REF].supabase.co:5432/postgres?sslmode=require

REDIS_URL=redis://localhost:6379
RABBITMQ_URL=amqp://chatwoot:chatwoot123@localhost:5672/

# Storage (MinIO)
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_USE_SSL=false

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Server
PORT=8080
FRONTEND_URL=http://localhost:5173
GO_ENV=development

# Features
ENABLE_WEBHOOKS=true
ENABLE_EMAIL=false
